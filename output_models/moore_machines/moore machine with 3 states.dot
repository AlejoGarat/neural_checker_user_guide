digraph moore_machine {
	rankdir=LR size="8,5"
	node [shape=circle]
	"State A" [label="State A/0"]
	"State A" -> "State A" [label=a]
	"State A" -> "State B" [label=b]
	"State B" [label="State B/1"]
	"State B" -> "State C" [label=a]
	"State B" -> "State B" [label=c]
	"State C" [label="State C/1"]
	node [shape=point]
	start
	start -> "State A"
}
